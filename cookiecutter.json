{
  "project_name": "My SHOP",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '-') }}",
  "app_name": "{{ cookiecutter.project_slug|replace('-', '') }}",
  "appName": "{{ cookiecutter.project_name|replace('-', '')|replace(' ', '') }}",
  "description": "Blueprint for an e-commerce site based on django-SHOP",
  "author_name": "Jacob Rief",
  "domain_name": "example.com",
  "email": "{{ cookiecutter.author_name.lower()|replace(' ', '.') }}@{{ cookiecutter.domain_name }}",
  "version": "1.0.0",
  "timezone": "UTC",
  "windows": "n",
  "use_pycharm": "n",
  "use_docker": "n",
  "use_i18n": "y",
  "use_paypal": "n",
  "use_stripe": "y",
  "use_sendcloud": "y",
  "printable_invoice": "y",
  "delivery_handling": [
    "partial",
    "common",
    false
  ],
  "products_model": [
    "polymorphic",
    "smartcard",
    "commodity"
  ],
  "use_compressor": "n",
  "keep_local_envs_in_vcs": "y",
  "debug": "n"
}
